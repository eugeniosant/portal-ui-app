        <name>teachersIdRicerca</name>
        <tag-class>it.tecnit.unica.plugins.jpteachers.aps.tags.TeachersIdRicercaTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>L'idricerca del docente.</description>
            <name>var</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Restituisce l'idesse3 del docente in sessione.
        </description>
        <name>teachersIdEsse3</name>
        <tag-class>it.tecnit.unica.plugins.jpteachers.aps.tags.TeachersIdEsse3Tag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>L'idesse3 del docente.</description>
            <name>var</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
	<tag>
