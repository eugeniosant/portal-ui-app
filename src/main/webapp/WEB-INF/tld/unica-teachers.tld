<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.1" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd">
    <description>"UniCa Teachers tag library"</description>
    <display-name>"UniCa Teachers tag library"</display-name>
    <tlib-version>1.0</tlib-version>
    <short-name>teachers</short-name>
    <uri>/unica-teachers</uri>
    <tag>
        <description>Restituisce l'ultimo giorno del mese attuale.
        </description>
        <name>lastDayOfMonth</name>
        <tag-class>it.tecnit.unica.plugins.jpteachers.aps.tags.LastDayOfMonthTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>L'ultimo giorno del mese.</description>
            <name>var</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Restituisce il contentId del docente in sessione o passato attraverso parametro.
        </description>
        <name>teachersContentId</name>
        <tag-class>it.tecnit.unica.plugins.jpteachers.aps.tags.TeachersContentIdTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Il contentid del docente.</description>
            <name>var</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description>L'identificativo del docente</description>
            <name>teacherId</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Restituisce il sito del docente in sessione.
        </description>
        <name>teachersSite</name>
        <tag-class>it.tecnit.unica.plugins.jpteachers.aps.tags.TeachersSiteTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Il sito del docente.</description>
            <name>var</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Restituisce il gruppo del docente in sessione.
        </description>
        <name>teachersGroup</name>
        <tag-class>it.tecnit.unica.plugins.jpteachers.aps.tags.TeachersGroupTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Il gruppo del docente.</description>
            <name>var</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>
            Loads a list of contents IDs by applying the filters (if any).
            Only the IDs of the contents accessible in the portal can be loaded.
        </description>
        <name>contentList</name>
        <tag-class>it.tecnit.unica.plugins.jpteachers.aps.tags.TeachersContentListTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>
                Name of the variable in the page context that holds the search result.
            </description>
            <name>listName</name>
            <required>true</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description>
                Sets the code of the content types to search.&lt;br/&gt;
                The name must match the configured one, respecting capital letters and spaces.
            </description>
            <name>contentType</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description>
                Identifier string of the category of the content to search.
            </description>
            <name>category</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description>
                Identifier string of the group of the content to search.
            </description>
            <name>group</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description>
                Toggles the system caching usage when retrieving the list.
                Admitted values (true|false), default "true".
            </description>
            <name>cacheable</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inserts the title on a variable of the page context with the name provided.</description>
            <name>titleVar</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inserts the code of the page to link on a variable of the page context with the name provided.</description>
            <name>pageLinkVar</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inserts the description of the page to link on a variable of the page context with the name provided.</description>
            <name>pageLinkDescriptionVar</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of the variable in the page context that holds the user filter options</description>
            <name>userFilterOptionsVar</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Restituisce il contenuto del docente in sessione avente id esse 3.
